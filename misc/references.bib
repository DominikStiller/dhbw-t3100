@inproceedings{M.Roth.2011,
 abstract = {The second order extended Kalman filter (EKF2) is based on a second order Taylor expansion of a nonlinear system, in contrast to the more common (first order) extended Kalman filter (EKF1). Despite a solid theoretical ground for its approximation, it is seldom used in applications, where the EKF and the unscented Kalman filter (UKF) are the standard algorithms. One reason for this might be the requirement for analytical Jacobian and Hessian of the system equations, and the high complexity that scales with the state order nx as n5x. We propose a numerical algorithm which is based on an extended set of sigma points (compared to the UKF) that needs neither Jacobian nor Hessian (or numerical approximations of these). Further, it scales as n4x, which is an order of magnitude better than the EKF2 algorithm presented in literature.},
 author = {{M. Roth} and {F. Gustafsson}},
 title = {{An efficient implementation of the second order extended Kalman filter}},
 keywords = {analytical Jacobian matrix;Approximation algorithms;Approximation methods;Covariance matrix;Hessian matrices;Hessian matrix;Jacobian matrices;Kalman Filters;Noise;nonlinear system;second order extended Kalman filter;second order Taylor expansion;sigma points;Taylor series;Unscented Kalman Filter},
 pages = {1--6},
 booktitle = {{14th International Conference on Information Fusion}},
 year = {2011},
 file = {M. Roth, F. Gustafsson 2011 - An efficient implementation:A\:\\dev\\dhbw-t3100\\references\\M. Roth, F. Gustafsson 2011 - An efficient implementation.pdf:pdf}
}


